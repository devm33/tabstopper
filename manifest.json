{
    "manifest_version": 2,
    "name": "tabstopper",
    "version": "0.1",
    "background": {
        "persistent": false,
        "page": "background.html"
    },
    "page_action": {
        "default_icon": {
            "19": "images/icon19.png",
            "38": "images/icon38.png"
        },
        "default_title": "This page is open in multiple tabs",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "webNavigation"
    ]
}
