{
    "manifest_version": 2,
    "name": "tabstopper",
    "version": "0.1",
    "icons": {
        "128": "icon_128.png"
    },
    "background": {
        "persistent": false,
        "page": "background.html"
    },
    "page_action": {
        "default_title": "This page is open in multiple tabs",
        "default_popup": "popup.html"
    },
    "permissions": [
        "tabs",
        "storage"
    ]
}
